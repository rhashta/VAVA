package model;

import java.io.Serializable;

import javax.persistence.Id;

public class Subject implements Serializable {
	
	@Id
	private int id;
	private String name;
	
	public Subject(){
		
	}
	
	   public int getId() {
	      return id;
	   }

	   public void setId(int id) {
	      this.id = id;
	   }

	   public String getName() {
	      return name;
	   }

	   public void setName(String name) {
	      this.name = name;
	   }

}
